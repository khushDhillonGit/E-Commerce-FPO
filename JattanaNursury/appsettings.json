{
  //Minimum level to log in these settings
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    //structural logging
    "Enrich": ["FromLogContext","WithMachineName","WithProcessId","WithThreadId"]
  },
 
  "AllowedHosts": "*"
}
